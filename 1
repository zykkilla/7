#include <stdio.h>
#include <string.h>

int max(int a, int b) {
    if (a > b) {
        return a;
    } else {
        return b;
    }
}

int main() {
    int num1, num2, maxnum;

    printf("Введите два числа: ");
    scanf("%d %d", &num1, &num2);

    maxnum = max(num1, num2);

    printf("Наибольшее число: %d\n", maxnum);

    return 0;
}
