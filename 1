#include <stdio.h>
#include <string.h>

int max(int a, int b) {
    if (a > b) {
        printf("Максимальное число: %d\n", a);
        return a;
    } else {
        printf("Максимальное число: %d\n", b);
        return b;
    }
}

int main() {
    int num1, num2, maximum;

    printf("Введите два числа: ");
    scanf("%d %d", &num1, &num2);

    maximum = max(num1, num2);

    printf("Максимальное число: %d\n", maximum);

    return 0;
}
